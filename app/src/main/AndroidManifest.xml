<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jms10.langstudykbrd">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service
            android:name=".CustomKeyboard.SoftKeyboard"
            android:permission="android.permission.BIND_INPUT_METHOD">
            <meta-data
                android:name="android.view.im"
                android:resource="@xml/method" />

            <intent-filter>
                <action android:name="android.view.InputMethod" />
            </intent-filter>
        </service>
        <service
            android:name=".Notification.NotificationService"
            android:hardwareAccelerated="false" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DictionaryActivity"
            android:label="Simple Dictionary"
            android:theme="@style/Theme.AppCompat.Light.Dialog" />
        <activity
            android:name=".WordSelectActivity"
            android:label="Select word"
            android:theme="@style/Theme.AppCompat.Light.Dialog" />
        <activity android:name=".SettingMenu" />
        <activity android:name=".WordSelectActivity" />
        <activity android:name=".Search_Word" />
        <activity android:name=".Setting_Font" />
        <activity android:name=".SettingkbrActivity" />
        <activity android:name=".DictimeActivity" />
        <activity android:name=".PicActivity" />
        <activity android:name=".StudyagainActivity" />
        <activity android:name=".Setting_Push" />
        <activity android:name=".DirectInputActivity"></activity>
        <activity android:name=".SettingkbrActivity"/>
        <activity android:name=".SettingmenuActivity"/>
        <activity android:name=".SettingdictionaryActivity"/>
        <activity android:name=".SettingpushActivity"/>
    </application>

</manifest>