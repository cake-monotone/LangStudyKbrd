<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    android:key="settingdictionary">
    <PreferenceCategory
        android:key="setting_pic_on_off"
        android:title="사진 출력"
        android:contentDescription="사진 출력 On/Off">
        <SwitchPreference
            android:defaultValue="false"
            android:key="piconoff"
            android:summaryOn="사진을 출력 합니다"
            android:summaryOff="사진을 출력하지 않습니다"
            android:switchTextOff="OFF"
            android:switchTextOn="ON"
            android:title="사진 출력"
            />
    </PreferenceCategory>
    <PreferenceCategory
        android:key="fontsize"
        android:title="글씨 크기"
        android:contentDescription="글씨 크기">
        <Preference
            android:key="go_to_font"
            android:title="글씨 크기 조절"
            android:summary="글씨 크기를 조절합니다"/>
    </PreferenceCategory>
    <PreferenceCategory
        android:key="setting_cooltime"
        android:title="사전 쿨타임"
        android:contentDescription="사전 쿨타임">
        <ListPreference
            android:defaultValue="0"
            android:title="사전 쿨타임 설정"
            android:key="CoolTime"
            android:dependency="CoolTime"
            android:entries="@array/CoolTime"
            android:entryValues="@array/CoolTime_values"
            android:negativeButtonText="@null"
            android:positiveButtonText="@null"
            />
    </PreferenceCategory>


</PreferenceScreen>